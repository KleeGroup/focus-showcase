{
  "name": "focus-showcase",
  "version": "0.0.1",
  "description": "Focus showcase",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run bundle",
    "start": "npm run start:npm",
    "start:local": "better-npm-run webpack-dev-server-local-focus",
    "start:npm": "better-npm-run webpack-dev-server-npm-focus",
    "test": "mocha --timeout 20000"
  },
  "babel": {
    "presets": [
      "focus"
    ]
  },
  "betterScripts": {
    "bundle": {
      "command": "webpack --progress",
      "env": {
        "ANCHOR_CLASS": "focus-showcase",
        "BABELIFIED_PATH": "./src",
        "ENTRY_FILE_PATH": "./src/app",
        "PAGE_TITLE": "Focus showcase",
        "LOCAL_FOCUS": true,
        "GENERATE_HTML": false,
        "OUTPUT_DIR": ".",
        "OUTPUT_PUBLIC_PATH": "",
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "MINIMIFY": false
      }
    },
    "webpack-dev-server-local-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "focus-showcase",
        "BABELIFIED_PATH": "./src",
        "ENTRY_FILE_PATH": "./src/app",
        "PAGE_TITLE": "Focus showcase",
        "LOCAL_FOCUS": true,
        "GENERATE_HTML": true,
        "OUTPUT_DIR": "."
      }
    },
    "webpack-dev-server-npm-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "focus-showcase",
        "BABELIFIED_PATH": "./src",
        "ENTRY_FILE_PATH": "./src/app",
        "PAGE_TITLE": "Focus showcase",
        "LOCAL_FOCUS": false,
        "GENERATE_HTML": false,
        "OUTPUT_DIR": "."
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KleeGroup/focus-showcase.git"
  },
  "keywords": [
    "focus",
    "showcase",
    "components",
    "swag"
  ],
  "author": "focus@kleegroup.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KleeGroup/focus-showcase/issues"
  },
  "homepage": "https://github.com/KleeGroup/focus-showcase#readme",
  "components": [
    "common/form",
    "components/input/checkbox",
    "components/input/select-checkbox",
    "components/input/select-radio",
    "components/input/date",
    "components/input/select",
    "components/input/text",
    "components/input/textarea",
    "components/input/toggle",
    "components/input/autocomplete-select",
    "components/input/autocomplete-text",
    "components/layout",
    "components/menu",
    "components/message-center",
    "components/panel",
    "components/scrollspy-container",
    "application/popin",
    "components/confirm",
    "search/search-bar",
    "search/facet-box",
    "page/search/quick-search",
    "page/search/advanced-search",
    "components/button",
    "components/button-back-to-top",
    "components/grid",
    "list/summary",
    "components/progress-bar"
  ],
  "devDependencies": {
    "assert": "^1.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-focus": "^0.6.1",
    "babel-runtime": "^6.5.0",
    "babel-standalone": "^6.4.4",
    "backbone": "^1.3.3",
    "better-npm-run": "0.0.11",
    "brace": "^0.8.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "focus-components": "0.17.0-beta5",
    "focus-core": "0.16.0-cavimac-2.0",
    "font-awesome": "^4.5.0",
    "highlight.js": "^9.7.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.3",
    "lodash": "^4.16.4",
    "lunr": "^0.7.1",
    "material-design-icons-iconfont": "^3.0.1",
    "mocha": "^3.1.1",
    "moment": "^2.11.2",
    "node-sass": "^3.3.3",
    "numeral": "^1.5.3",
    "opener": "^1.4.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-ace": "^3.6.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "selenium-webdriver": "^3.0.0-beta-3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-focus": "^0.12.2"
  }
}
